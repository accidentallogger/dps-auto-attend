{
    "name": "dps",
    "version": "0.3",
    "manifest_version": 2,


    "content_scripts": [{
        "matches": ["<all_urls>"],
        "js": ["popup.js"]
    }],
    "background": {
        "scripts": ["bg.js"]
    },

    "browser_action": {

        "default_popup": "popup.html",
        "default_icon": "dip.png"
    },
    "permissions": ["storage"]
}